{
  "name": "Google Search Number",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["tabmove.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
